-- Orion Library laden
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- GUI erstellen
local Window = OrionLib:MakeWindow({
    Name = "FlixEx [BETA]",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "HCPDConfig"
})

-- Tabs erstellen
local MainTab = Window:MakeTab({
    Name = "Main ‚öô",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local AimLockTab = Window:MakeTab({
    Name = "AimLock üèπ",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local TeleportsTab = Window:MakeTab({
    Name = "Teleports ‚ú®",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local InfoTab = Window:MakeTab({
    Name = "Info ‚ÑπÔ∏è",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Info Tab-Inhalt hinzuf√ºgen
InfoTab:AddParagraph("Credits", "Made by Keys")

-- SpeedHack-Funktionalit√§t (im Main Tab)
local SpeedHackEnabled = false
local speedValue = 16

MainTab:AddToggle({
    Name = "SpeedHack",
    Default = false,
    Callback = function(Value)
        SpeedHackEnabled = Value
        if SpeedHackEnabled then
            game.Players.LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed = speedValue
        else
            game.Players.LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed = 16
        end
    end
})

MainTab:AddSlider({
    Name = "Speed",
    Min = 0,
    Max = 500,
    Default = 16,
    Callback = function(Value)
        speedValue = Value
        if SpeedHackEnabled then
            game.Players.LocalPlayer.Character:WaitForChild("Humanoid").WalkSpeed = speedValue
        end
    end
})

-- Teleport-Buttons im Teleports Tab
TeleportsTab:AddButton({
    Name = "Gate",
    Callback = function()
        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(-47.42, 5.60, -498.29))
    end
})

TeleportsTab:AddButton({
    Name = "Weapon Shop",
    Callback = function()
        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(-348.018798828125, 5.600290775299072, -778.2922873632812))
    end
})

TeleportsTab:AddButton({
    Name = "Safe Gate TP",
    Callback = function()
        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(-164.77700805664062, 6.316139221191406, -646.4644775390625))
    end
})

TeleportsTab:AddButton({
    Name = "Inside ASU area",
    Callback = function()
        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(-283.7239074707031, 6.4162373542785645, -201.4882354736328) + Vector3.new(0, 0, 5))
    end
})

-- AimLock Button und Paragraph
AimLockTab:AddButton({
    Name = "Execute AimLock",
    Callback = function()
        -- Der AimLock Code kann hier integriert werden
       local Players = game:GetService("Players")
    local UserInputService = game:GetService("UserInputService")
    local RunService = game:GetService("RunService")

    -- Variablen f√ºr AimLock
    local aimLockEnabled = false
    local predictionEnabled = false
    local predictionAmount = 0.1 -- Anpassbare Prediction
    local lockedPlayer = nil

    -- GUI erstellen
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "AimLockGui"
    screenGui.ResetOnSpawn = false -- GUI bleibt bestehen, auch nach dem Tod
    screenGui.Parent = Players.LocalPlayer:WaitForChild("PlayerGui")

    -- Haupt-Frame
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0.25, 0, 0.2, 0)
    mainFrame.Position = UDim2.new(0.375, 0, 0.4, 0)
    mainFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    mainFrame.BorderSizePixel = 0
    mainFrame.Parent = screenGui
    mainFrame.Active = true
    mainFrame.Draggable = true

    -- Runde Ecken f√ºr Frame
    local mainFrameCorner = Instance.new("UICorner")
    mainFrameCorner.CornerRadius = UDim.new(0, 12)
    mainFrameCorner.Parent = mainFrame

    -- √úberschrift
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Text = "AimLock System"
    titleLabel.Size = UDim2.new(1, 0, 0.3, 0)
    titleLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextScaled = true
    titleLabel.Parent = mainFrame

    -- Runde Ecken f√ºr Titel
    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 12)
    titleCorner.Parent = titleLabel

    -- AimLock-Button
    local aimLockButton = Instance.new("TextButton")
    aimLockButton.Text = "AimLock: OFF"
    aimLockButton.Size = UDim2.new(0.8, 0, 0.25, 0) -- Angepasste H√∂he
    aimLockButton.Position = UDim2.new(0.1, 0, 0.35, 0) -- Zentriert
    aimLockButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    aimLockButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    aimLockButton.Font = Enum.Font.GothamBold
    aimLockButton.TextScaled = true
    aimLockButton.Parent = mainFrame

    -- Runde Ecken f√ºr AimLock-Button
    local aimLockCorner = Instance.new("UICorner")
    aimLockCorner.CornerRadius = UDim.new(0, 8)
    aimLockCorner.Parent = aimLockButton

    -- Prediction-Button
    local predictionButton = Instance.new("TextButton")
    predictionButton.Text = "Prediction: OFF"
    predictionButton.Size = UDim2.new(0.8, 0, 0.25, 0) -- Gleiche H√∂he wie AimLock
    predictionButton.Position = UDim2.new(0.1, 0, 0.7, 0) -- Korrekte Position
    predictionButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    predictionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    predictionButton.Font = Enum.Font.GothamBold
    predictionButton.TextScaled = true
    predictionButton.Parent = mainFrame

    -- Runde Ecken f√ºr Prediction-Button
    local predictionCorner = Instance.new("UICorner")
    predictionCorner.CornerRadius = UDim.new(0, 8)
    predictionCorner.Parent = predictionButton

    -- Funktionen f√ºr Kn√∂pfe
    aimLockButton.MouseButton1Click:Connect(function()
        aimLockEnabled = not aimLockEnabled
        if aimLockEnabled then
            aimLockButton.Text = "AimLock: ON"
            aimLockButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        else
            aimLockButton.Text = "AimLock: OFF"
            aimLockButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            lockedPlayer = nil -- AimLock zur√ºcksetzen
        end
    end)

    predictionButton.MouseButton1Click:Connect(function()
        predictionEnabled = not predictionEnabled
        if predictionEnabled then
            predictionButton.Text = "Prediction: ON"
            predictionButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        else
            predictionButton.Text = "Prediction: OFF"
            predictionButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        end
    end)

    -- Hilfsfunktion, um den n√§chsten Spieler zu finden
    local function getClosestPlayerToMouse()
        local mousePos = UserInputService:GetMouseLocation()
        local closestPlayer = nil
        local closestDistance = math.huge

        for _, player in pairs(Players:GetPlayers()) do
            if player ~= Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local character = player.Character
                local screenPos, onScreen = workspace.CurrentCamera:WorldToViewportPoint(character.HumanoidRootPart.Position)
                if onScreen then
                    local distance = (Vector2.new(screenPos.X, screenPos.Y) - mousePos).Magnitude
                    if distance < closestDistance then
                        closestDistance = distance
                        closestPlayer = player
                    end
                end
            end
        end

        return closestPlayer
    end

    -- AimLock aktivieren/deaktivieren
    UserInputService.InputBegan:Connect(function(input, isProcessed)
        if isProcessed then return end
        if input.KeyCode == Enum.KeyCode.C then
            if lockedPlayer then
                -- AimLock deaktivieren
                lockedPlayer = nil
            else
                -- Den n√§chsten Spieler in der N√§he der Maus finden
                lockedPlayer = getClosestPlayerToMouse()
            end
        end
    end)

    -- Maus auf das Ziel richten
    RunService.RenderStepped:Connect(function()
        if aimLockEnabled and lockedPlayer and lockedPlayer.Character and lockedPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local targetPos = lockedPlayer.Character.HumanoidRootPart.Position
            if predictionEnabled then
                local rootVelocity = lockedPlayer.Character.HumanoidRootPart.Velocity
                targetPos = targetPos + rootVelocity * predictionAmount
            end

            local camera = workspace.CurrentCamera
            camera.CFrame = CFrame.new(camera.CFrame.Position, targetPos)
        end
    end)
end
})

AimLockTab:AddParagraph(
    "‚ö†Ô∏èImportant‚ö†Ô∏è",
    "Only use prediction when you\nfight at high range\nLock/Unlock with C"
)

-- GUI starten
OrionLib:Init()
